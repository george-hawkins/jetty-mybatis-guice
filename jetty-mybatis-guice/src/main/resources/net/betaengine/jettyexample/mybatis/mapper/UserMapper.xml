<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.betaengine.jettyexample.mybatis.mapper.UserMapper">

  <select id="getUser" resultType="net.betaengine.jettyexample.mybatis.domain.User">
    select * from users where id=#{value}
  </select>
  
  <insert id="brokenAdd" useGeneratedKeys="true" keyProperty="id">
    INSERT INTO
      contact (id,
        name
      )
    VALUES (
      #{name}
    )
  </insert>

</mapper>
